{
    "name": "y.miatselitsa/aks",
    "autoload": {
        "psr-4": {
            "YMiatselitsa\\Aks\\": "src/"
        }
    },
    "authors": [
        {
            "name": "y.miatselitsa",
            "email": "y.miatselitsa@expondo.com"
        }
    ],
    "require": {
        "symfony/yaml": "^7.1",
        "symfony/console": "^7.1",
        "symfony/dotenv": "^7.1",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "^7.1",
        "symfony/property-access": "^7.1",
        "symfony/runtime": "^7.1",
        "symfony/serializer": "^7.1",
        "php": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
